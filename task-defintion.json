{
    "family": "flyway",
	"containerDefinitions": [
		{
			"name": "flyway-container",
			"image": "grocery-app-flyway",
			"essential": true,
			"portMappings": [
				{
					"containerPort": 3306,
					"hostPort": 3306
				}
			]
		}
	],
	"cpu": "256",
	"memory": "512",
	"networkMode": "awsvpc",
	"executionRoleArn": "arn:aws:iam::417297593891:role/grocery-app-execution-task-role",
	"requiresCompatibilities": [
       "FARGATE"
    ]
}
